} */

.changePage{
  width: 750rpx;
  height: 100rpx;
  line-height: 50rpx;
  margin-top: 30rpx;
  /* margin-bottom: 120rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: white;
}

.changePage .prePage-btn{
  width: 375rpx;
  text-align: center;
  border-right: solid #b5b5b5 2rpx;
}

.changePage .nextPage-btn{
  width: 375rpx;
  text-align: center;
}