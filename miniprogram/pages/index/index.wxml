<!-- miniprogram/pages/index/index.wxml -->
<view class="index-main">
  <!-- 轮播图 -->
  <swiper circular="true" indicator-color="rgba(0, 0, 0, .3)" indicator-active-color="#fff" indicator-dots class="index-swiper" autoplay="{{true}}">
    <swiper-item>
      <image mode="aspectFit" class="swiper-img" src="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/banner/banner1.png" />
    </swiper-item>
    <swiper-item>
      <image mode="aspectFit" bindtap="toNotice" class="swiper-img" src="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/banner/谷雨小清新插画公众号封面.png" />
    </swiper-item>
    <swiper-item>
      <image mode="aspectFit" class="swiper-img" bindtap="toNotice" src="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/banner/广告位招租.png" />
    </swiper-item>
  </swiper>
  <!-- 模块 -->
  <!-- 第一行 -->
  <view class="index-list">
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/水电.png" pageTo="water" msg="水电查询"></indexItem>
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/图书馆.png" msg="图书馆" pageTo="urlCopy" option="图书馆"></indexItem>
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/外卖.png" msg="外卖推荐" pageTo="sendFood"></indexItem>
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/校历.png" msg="校历"></indexItem>
  </view>
  <!-- 第二行 -->
  <view class="index-list">
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/地图.png" msg="地图"></indexItem>
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/墙.png" msg="讨论墙" pageTo="wall"></indexItem>
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/宣传.png" msg="宣传申请" pageTo="urlCopy" option='宣传申请'></indexItem>
    <indexItem imgUrl="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/场地预约.png" msg="场地预约" pageTo="urlCopy" option='场地预约'></indexItem>
  </view>
</view>

<view class="index-title">精选内容</view>
<view class="indexList1">
  <view class="list-left" bind:tap="toMiniProgram">
    <image src="cloud://test-4gbjp6opfa0ad4c1.7465-test-4gbjp6opfa0ad4c1-1304984739/icon/龙猫配送.png" />
  </view>
  <view class="list-right">
    <view class="right-top">
      <view class="schedule">
        <view class="msg">你的{{year}}还剩余{{100-schedule}}%</view>
        <view class="schedule-content" style="width:{{scheduleP}}rpx"></view>
      </view>
    </view>
    <view class="right-bottom"></view>
  </view>
</view>

<view class="index-title">新闻速览</view>
<view class="news-main">
  <swiper class="news-swiper" vertical="true" circular="true" autoplay="true">
    <swiper-item class="news-swiperItem" wx:for="{{newList}}">
      <image class="news-img" src="data:image/png;base64,"></image>
      <view class="news-text">{{item.title}}</view>
    </swiper-item>
  </swiper>
</view>
<tabBar chooseTag="1"></tabBar>