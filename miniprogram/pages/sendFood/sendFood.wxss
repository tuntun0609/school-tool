/* pages/sendFood/sendFood.wxss */

.sendFood-list{
  min-height: 100vh;
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
  background-color: #f5f6fa;
  padding-top: 20rpx;
}

.sendFood-list .sendFood-notice{
  width: 600rpx;
  background-color: #fff;
  /* border: solid 1rpx #4a69bd; */
  border-radius: 20rpx;
  box-shadow: 0 0 10rpx 0 rgba(0, 0, 0, 0.3);
  display: flex;
  align-items: center;
  margin: 0 auto;
  padding: 30rpx 30rpx;
  color: #192a56;
}

.foodItem:last-of-type{
  margin-bottom: 155rpx;
}
