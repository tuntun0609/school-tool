@import "./iconfont.wxss";

page {
  background: rgb(245, 246, 250);
  padding-top: 85rpx;
  width: 100%;
  height: 100%;
}

[class*=animation-] {
  animation-duration: .5s;
  animation-timing-function: ease-out;
  animation-fill-mode: both
}

.animation-slide-bottom {
  animation-name: slide-bottom
}

.image-animation-slide-bottom {
  animation-name: image-slide-bottom
}

.tips-show {
  animation-duration: 1s;
  animation-name: tips-show
}

@keyframes tips-show {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes image-slide-bottom {
  0% {
    opacity: 0;
    transform: translateY(20%)
  }

  100% {
    opacity: 1;
    transform: translateY(0)
  }
}

@keyframes slide-bottom {
  0% {
    opacity: .4;
    transform: translateY(7%)
  }

  100% {
    opacity: 1;
    transform: translateY(0)
  }
}

.search-container {
  position: relative;
  padding: 80rpx 0 0;
  background: #fff;
  border-radius: 40rpx;
  box-shadow: 0rpx 0.1rpx 8rpx 0.1rpx rgb(64 60 67 / 8%);
}

.search-container::before {
  position: absolute;
  content: '\e614';
  display: block;
  background: rgb(250, 252, 255);
  width: 125rpx;
  height: 125rpx;
  line-height: 125rpx;
  text-align: center;
  border-radius: 50%;
  color: rgb(74, 144, 226);
  font-size: 88rpx;
  top: 0;
  left: 50%;
  transform: translate(-50%, -50%);
  border: solid 10rpx rgb(138, 220, 252);
}

.search-wrap {
  padding: 0 25rpx;
  height: 400rpx;
}

.search-container .title {
  text-align: center;
  font-size: 64rpx;
  font-weight: 600;
  letter-spacing: 2rpx;
  color: rgb(53, 105, 209);
}

picker,
input[type="text"] {
  position: relative;
  border-radius: 20rpx;
  height: 80rpx;
  padding: 0 60rpx;
  margin-top: 25rpx;
  background-color: rgb(245, 249, 252);
  box-shadow: 0 0.1px 3px 0.1px rgb(64 60 67 / 10%);
  font-size: 34rpx;
}

.choseQuestion::before,
input[type="text"]::before {
  position: absolute;
  left: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  color: rgba(189, 189, 203, 0.774);
  font-size: 36rpx;
}

picker .choseQuestion {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  color: rgba(189, 189, 203, 0.774);

}

picker .choice {
  font-size: 34rpx;
  color: #000;
}

#form1 .place-holder {
  font-size: 34rpx;
  color: rgba(189, 189, 203, 0.774)
}

.button-wrap {
  position: relative;
  text-align: center;
}

.search-container .search-btn,
.search-container .reset-btn {
  position: absolute;
  display: inline-block;
  padding: 0;
  width: 38%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 90rpx;
  margin-top: 60rpx;
  font-size: 36rpx;
  text-align: center;
  font-weight: 520;
  color: #fff;
}

.search-container .search-btn {
  right: 8%;
  background-color: rgb(0, 174, 239);
  box-shadow: 0 1px 5px 0.1px rgb(0 174 239 / 16%);
}

.search-container .reset-btn {
  left: 8%;
  box-shadow: 0 0px 5px 0.1px rgb(64 60 67 / 16%);
  background-color: rgb(252, 91, 91);
}

.search-container .reset-btn:active,
.search-container .search-btn:active {
  background-color: rgba(97, 82, 82, 0.452);
}

.search-container .reset,
.search-container .search {
  font-size: 36rpx;
}

.result-wrap {
  padding: 20rpx;
  border-top: 1px dashed rgba(150, 146, 146, 0.26);
  height: 850rpx;
}

.result-wrap .result-container {
  box-shadow: 0rpx 0.1rpx 8rpx 0.1rpx rgb(64 60 67 / 10%);
}

.result-wrap .result-header {
  position: relative;
  height: 80rpx;
}

.result-header .result-title {
  font-weight: 520;
  margin-left: 65rpx;
  font-size: 46rpx;
  color: rgba(0, 0, 0, 0.76);
}

.result-header::before {
  position: absolute;
  top: -2rpx;
  font-size: 64rpx;
}

.result-container {
  border-radius: 20rpx;
  overflow: hidden;
}

.result-main {
  display: flex;
}

.result-main .room,
.result-main .balance {
  display: flex;
  flex: 1;
  height: 260rpx;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  background-color: rgb(66, 165, 245);
  color: rgb(250, 253, 253);
  font-weight: 520;
  font-size: 38rpx;
}

.room::before,
.balance::before {
  font-size: 64rpx;
  margin-bottom: 8rpx;
}

.result-main .user-detail {
  margin-top: 5rpx;
  text-align: right;
  font-size: 32rpx;
  color: rgb(188, 206, 212);
}

.result-main .balance .insufficient {
  color: rgb(240, 72, 72);
}

.bonus-item {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid rgba(153, 150, 145, 0.24);
  padding: 20rpx 30rpx;
  font-size: 36rpx;
  color: rgba(0, 0, 0, 0.76);
}

.bonus-item .progress {
  position: absolute;
  left:30%;
  border-radius: 70rpx;
  width: 345rpx;
  height: 38rpx;
  font-weight: 600;
  overflow: hidden;
  background-color: #ebeef5;
}
.progress .bar{
  height: 100%;
  width: 0;
  transition: width 1s ease;
}
.progress text{
  display: block;
  position: absolute;
  height:100%;
  line-height: 38rpx;
  color: rgba(27, 27, 27, 0.705);
  font-size: 20rpx;
  top:0;
  left:50%;
  transform:translateX(-50%)
}

.bonus-item .electricity {
  color: rgb(231, 168, 30);
  /* background-color: rgb(252, 232, 174); */
}

.bonus-item .hot {
  color: rgb(231, 61, 46);
}

.bonus-item .cold {
  color: rgb(0, 172, 237);
  /*   background-color: rgb(183, 223, 241); */
}

.bonus .search-time {
  display: block;
  text-align: right;
  color: rgba(153, 150, 145, 0.61);
  font-style: italic;
  font-size: 30rpx;
  border: none;
}

.footer {
  position: relative;
  bottom: 0;
  width: 100%;
  text-align: center;
}

.footer .tips {
  position: absolute;
  font-size: 50rpx;
  width: 100%;
  left: 50%;
  top: 10%;
  transform: translateX(-50%);
  color: rgb(37, 41, 92);
  font-weight: 530;
  letter-spacing: 2rpx;
}

.footer .pic {
  display: inline-block;
  position: relative;
  bottom: -10rpx;
  height: 800rpx;
  width: 100%;
  background-position: center center;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}