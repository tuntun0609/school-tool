<!-- components/tabBar/tabBar.wxml -->
<view class="tabBar">
  <view class="tabBar-list">
    <view wx:if="{{chooseTag==1}}" class="index tabBar-item" bindtap="indexClick">
      <image src="data:image/png;base64," class="tabBar-img"/>
      <!-- <view class="text">首页</view> -->
    </view>
    <view wx:else class="index tabBar-item" bindtap="indexClick">
      <image src="data:image/png;base64," class="tabBar-img"/>
      <!-- <view class="text">首页</view> -->
    </view>

    <view wx:if="{{chooseTag==2}}" class="notice tabBar-item" bindtap="noticeClick">
      <image src="data:image/png;base64," class="tabBar-img"/>
      <!-- <view class="text">公告</view> -->
    </view>
    <view wx:else class="notice tabBar-item" bindtap="noticeClick">
      <image src="data:image/png;base64," class="tabBar-img"/>
      <!-- <view class="text">公告</view> -->
    </view>

    <view wx:if="{{chooseTag==3}}" class="user tabBar-item" bindtap="userClick">
      <image src="data:image/png;base64," class="tabBar-img"/>
      <!-- <view class="text">个人中心</view> -->
    </view>
    <view wx:else class="user tabBar-item" bindtap="userClick">
      <image src="data:image/png;base64," class="tabBar-img"/>
      <!-- <view class="text">个人中心</view> -->
    </view>
  </view>
</view>